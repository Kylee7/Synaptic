{
  "name": "synaptic",
  "version": "1.0.0",
  "description": "Decentralized AI Memory Connection Protocol - Making AI Truly Yours",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "ai",
    "memory",
    "protocol",
    "decentralized",
    "blockchain",
    "privacy",
    "claude",
    "openai",
    "gemini",
    "cursor",
    "windsurf"
  ],
  "author": "Synaptic Team",
  "license": "MIT",
  "homepage": "https://synapticmcp.xyz",
  "repository": {
    "type": "git",
    "url": "https://github.com/Synaptic-MCP/Synaptic.git"
  },
  "bugs": {
    "url": "https://github.com/Synaptic-MCP/Synaptic/issues"
  },
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postbuild": "npm run copy-assets",
    "copy-assets": "copyfiles -u 1 src/**/*.json dist/",
    "api": "npm run build && node dist/api/server.js",
    "setup": "npm install && npm run build",
    "setup:full": "ts-node src/scripts/setup.ts",
    "setup:dev": "ts-node src/scripts/setup.ts --environment development",
    "setup:prod": "ts-node src/scripts/setup.ts --environment production",
    "examples": "ts-node src/examples/basic-usage.ts",
    "examples:basic": "ts-node src/examples/basic-usage.ts",
    "docker:build": "docker build -t synaptic .",
    "docker:run": "docker run -p 3000:3000 synaptic",
    "docker:dev": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docs": "typedoc --out docs src",
    "docs:serve": "npx http-server docs -p 8080",
    "benchmark": "ts-node scripts/benchmark.ts",
    "migrate": "ts-node scripts/migrate.ts",
    "seed": "ts-node scripts/seed.ts",
    "health": "curl http://localhost:3000/health || echo 'Server not running'",
    "check": "npm run lint && npm run test && npm run build",
    "validate": "npm run check && npm run examples",
    "reset": "npm run clean && rm -rf node_modules package-lock.json && npm install",
    "update-deps": "npx npm-check-updates -u && npm install"
  },
